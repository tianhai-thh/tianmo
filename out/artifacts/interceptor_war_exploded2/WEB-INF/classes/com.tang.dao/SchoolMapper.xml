<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.tang.dao.SchoolMapper">


    <select id="selectcsid" resultType="java.lang.Integer">

        select id from school where schoolname = #{schoolname};

    </select>

    <select id="selectAll" resultType="School">
        select * from school;
    </select>


</mapper>